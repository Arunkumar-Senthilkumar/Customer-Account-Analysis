{
	"name": "ds_backenddata",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_backenddata",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder": {
				"type": "string"
			},
			"date": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().folder, '/', dataset().date)\r\n",
					"type": "Expression"
				},
				"container": "data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "account_id",
				"type": "String"
			},
			{
				"name": "customer_id",
				"type": "String"
			},
			{
				"name": "account_type",
				"type": "String"
			},
			{
				"name": "balance",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}